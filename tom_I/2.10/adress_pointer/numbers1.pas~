program Numbers1;           { numbers1.pas }
type
   itemptr = ^item;
   item	   = record
	  data : integer;
	  next : itemptr;
   end;
var
   first, tmp : itemptr;
   n		 : integer;
begin
   first := nil;            { Ğ´ĞµĞ»Ğ°ĞµĞ¼ ÑĞ¿Ğ¸ÑĞ¾Ğº ĞºĞ¾Ñ€Ñ€ĞµĞºÑ‚Ğ½Ğ¾ Ğ¿ÑƒÑÑ‚Ñ‹Ğ¼! }
   while not SeekEof do     { Ñ†Ğ¸ĞºĞ» Ñ‡Ñ‚ĞµĞ½Ğ¸Ñ Ñ‡Ğ¸ÑĞµĞ» }
   begin
      read(n);
      new(tmp);             { ÑĞ¾Ğ·Ğ´Ğ°Ğ»Ğ¸ }
      tmp^.data := n;       { Ğ·Ğ°Ğ¿Ğ¾Ğ»Ğ½ÑĞµĞ¼ }
      tmp^.next := first;
      first := tmp;         { Ğ²ĞºĞ»ÑÑ‡Ğ°ĞµĞ¼ Ğ² ÑĞ¿Ğ¸ÑĞ¾Ğº }
   end;
   tmp := first;            { Ğ¿Ñ€Ğ¾Ñ…Ğ¾Ğ´Ğ¸Ğ¼ Ğ¿Ğ¾ ÑĞ¿Ğ¸ÑĞºÑƒ Ñ Ğ½Ğ°Ñ‡Ğ°Ğ»Ğ° }
   while tmp <> nil do      { Ğ´Ğ¾ ĞºĞ¾Ğ½Ñ†Ğ° }
   begin
      writeln(tmp^.data);
      tmp := tmp^.next      { Ğ¿ĞµÑ€ĞµÑ…Ğ¾Ğ´Ğ¸Ğ¼ Ğº ÑĞ»ĞµĞ´ÑƒÑÑ‰ĞµĞ¼Ñƒ ÑĞ»-Ñ‚Ñƒ }
   end
end.
