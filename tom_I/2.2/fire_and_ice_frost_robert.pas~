program fire_and_ice;
begin
   writeln('Some say the world will end in fire,');
   read();
   writeln('Some say in ice.');
   read();
   writeln('From what I"ve tasted of desire');
   read();
   writeln('I hold with those who favor fire.');
   read();
   writeln('But if it had to perish twice,');
   read();
   writeln('I think I know enough of hate');
   read();
   writeln('To say that for destruction ice');
   read();
   writeln('Is also great');
   read();
   writeln('And would suffice.')
end.
